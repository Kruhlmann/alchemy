{
  "name": "alchemy-vscode-syntax",
  "displayName": "Alchemy VSCode Syntax",
  "description": "Syntax highlighting for Alchemy",
  "version": "0.0.1",
  "publisher": "Andreas Kr√ºhlmann",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "alchemy",
        "aliases": ["alchemy"],
        "extensions": [".alc"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "alchemy",
        "scopeName": "source.alc",
        "path": "./alchemy.tmLanguage.json"
      }
    ]
  }
}
