sub mul
    over 0 < if 
        rev3 0 rev3
        while clone 0 < do
            rev3 swap clone rev3 swap -
            rev3 ++
        wend
    else
        rev3 0 rev3
        while clone 0 > do
            rev3 swap clone rev3 +
            rev3 --
        wend
    endif
    drop drop
    swap
marine